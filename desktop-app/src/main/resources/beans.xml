<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="sp" class="com.angelcalvo.superpalitos.SuperPalitos" init-method="init">
		<property name="frame">
			<ref bean="frame" />
		</property>
		<property name="confManager">
			<ref bean="confManager"/>
		</property>
	</bean>
	<bean id="resourceManager" class="com.angelcalvo.superpalitos.ResourceManager">
	</bean>
	<bean id="frame" class="com.angelcalvo.superpalitos.gui.SPFrame" init-method="init">
		<property name="sp">
			<ref bean="sp"/>
		</property>
		<property name="resourceManager">
			<ref bean="resourceManager"/>
		</property>
		<property name="confManager">
			<ref bean="confManager"/>
		</property>
	</bean>
	<bean id="confManager" class="com.angelcalvo.superpalitos.ConfManager">
	</bean>
</beans>
